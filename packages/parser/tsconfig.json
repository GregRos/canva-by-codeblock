{
	"compilerOptions": {
		"inlineSourceMap": true,
		"inlineSources": true,
		"declaration": true,
		"declarationMap": true,
		"module": "NodeNext",
		"target": "ES2020",
		"noImplicitAny": true,
		"moduleResolution": "nodenext",
		"isolatedModules": true,
		"strictNullChecks": true,
		"lib": ["ES7", "DOM"]
	},
	"include": ["**/*.ts"]
}
